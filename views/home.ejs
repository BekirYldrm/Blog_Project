 <%- include ("partials/header") -%>

 <% if(posts.length === 0 ){ %>
 <h1>Home</h1>
 <p><%= homeContent %></p>
 <% } %>


 <% posts.forEach(post => {%>
 <h1><%=post.title%></h1>
 <p><%=post.content?.substring(0,50)%> ...
   <a href="/post/<%=post.title%>">Read More</a>
 </p>



 <% }); %>





 <%- include ("partials/footer") -%>